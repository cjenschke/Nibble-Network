
{{> navbar }}

<div class="container mt-4">
    <h1>Random Recipes</h1>
    <div>
        {{#each recipes}}
            <div class="mb-2">
                <img src="{{this.image}}" alt="">
                <p></p><a href="{{this.sourceUrl}}" class="text-decoration-none">{{this.title}}</a></p>
            </div>
            
        {{/each}}
    </div>
</div>



